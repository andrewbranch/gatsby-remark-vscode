{
  "name": "gatsby-remark-vscode",
  "version": "1.0.0",
  "description": "Gatsby plugin to provide VS Code language support and themes to code snippets",
  "main": "index.js",
  "scripts": {
    "build": "node scripts/scrapeBuiltinExtensions.js",
    "test": "jest",
    "pretest": "npm run build && prettier --check",
    "prepare": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrewbranch/gatsby-remark-vscode.git"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "remark",
    "syntax",
    "highlighting",
    "markdown"
  ],
  "author": "Andrew Branch <andrew@wheream.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andrewbranch/gatsby-remark-vscode/issues"
  },
  "homepage": "https://github.com/andrewbranch/gatsby-remark-vscode#readme",
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.13.7",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "glob": "^7.1.3",
    "jest": "^24.5.0",
    "prettier": "^1.17.0",
    "typescript": "^3.3.4000"
  },
  "dependencies": {
    "comment-json": "^1.1.3",
    "decompress": "^4.2.0",
    "lodash.escape": "^4.0.1",
    "lodash.uniq": "^4.5.0",
    "oniguruma": "^7.0.2",
    "plist": "^3.0.1",
    "vscode-textmate": "^4.1.0"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "lib"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/vscode"
    ]
  }
}
