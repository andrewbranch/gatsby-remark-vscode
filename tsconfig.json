{
  "compilerOptions": {
    "target": "es2015",
    "module": "commonjs",
    "allowJs": true,
    "checkJs": true,
    "declaration": true,
    "emitDeclarationOnly": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  },
  "include": [
    "gatsby-node.js",
    "src",
    "test",
    "lib/vscode",
    "scripts"
  ],
  "exclude": [
    "test/graphql/site"
  ]
}
